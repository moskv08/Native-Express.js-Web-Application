{
  "name": "wepapp",
  "version": "1.0.0",
  "description": "A web application build with Node.js and Express",
  "main": "app.js",
  "scripts": {
    "test": "mocha './test/**/*.spec.js'",
    "start": "DEBUG=app:* nodemon app.js",
    "starwin": "set DEBUG=app, app:* & nodemon app.js",
    "lint": "./node_modules/.bin/eslint app.js src/routes/**"
  },
  "keywords": [
    "node"
  ],
  "author": "Fabrizio Zihlmann",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^3.4.1",
    "chalk": "^2.4.2",
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "dotenv": "^8.1.0",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.16.2",
    "jquery": "^3.4.1",
    "morgan": "^1.9.1",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^7.12.1"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^6.2.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-plugin-import": "^2.18.2",
    "mocha": "^10.2.0",
    "nodemon": "^1.19.1",
    "sinon": "^7.4.2"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      "node_modules/**/node_modules"
    ],
    "delay": "2000",
    "env": {
      "NODE_ENT": "development",
      "PORT": "4000"
    }
  }
}
